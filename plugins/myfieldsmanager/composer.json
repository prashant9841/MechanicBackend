{
    "name": "yourname/myfieldsmanager",
    "description": "A Craft 4 plugin",
    "type": "craft-plugin",
    "require": {
      "craftcms/cms": "^4.0"
    },
    "autoload": {
      "psr-4": {
        "yournamespace\\myfieldsmanager\\": "src/"
      }
    },
    "extra": {
      "craft": {
        "handle": "myfieldsmanager",
        "class": "yournamespace\\myfieldsmanager\\MyFieldsManager"
      }
    }
  }
  